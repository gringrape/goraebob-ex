{
  "name": "react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "mkdir -p dist && concurrently 'npm:serve' 'npm:build -- --watch'",
    "serve": "servor dist index.html 19100 --reload",
    "build": "ts-node ./scripts/build.ts",
    "check": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "test": "jest --verbose && npm run codeceptjs:headless && npm run check",
    "watch": "jest --watchAll",
    "test:ci-e2e": "concurrently -k -s first 'npm run serve' 'sleep 5 && npm run codeceptjs:headless'",
    "codeceptjs": "codeceptjs run --steps",
    "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
    "codeceptjs:ui": "codecept-ui --app"
  },
  "author": "seedwhale",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.5.2",
    "subscriptions-transport-ws": "^0.9.19",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@codeceptjs/configure": "^0.7.0",
    "@codeceptjs/ui": "^0.4.6",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "codeceptjs": "^3.2.3",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.11",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "playwright": "^1.17.2",
    "servor": "^4.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.6.0",
    "typescript": "^4.4.4"
  }
}
